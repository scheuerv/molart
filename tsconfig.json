{
  "compilerOptions": {
    "composite": false,
    "target": "es5",
    "outDir": "build/src",
    "jsx": "react",
    "sourceMap": true,
    "strictNullChecks": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "baseUrl": ".",
    "alwaysStrict": false,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "noUnusedLocals": false,
    "strictFunctionTypes": false,
    "allowUmdGlobalAccess": true,
    "paths": {
      "Molstar/*": [
        "node_modules/molstar/lib/*"
      ]
    },
    "allowJs": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}